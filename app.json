{
  "expo": {
    "name": "Event Flow",
    "slug": "taskManagement",
    "scheme": "Auth-Development",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/EF.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/EF.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": ["./assets/notification.wav", "./assets/notification.wav"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/EF.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
      // "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/EF.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleSignIn": {
          "certificateHash": "<YOUR_ANDROID_CERTIFICATE_HASH>"
        }
      }
    },
    "web": {
      "favicon": "./assets/EF.png"
    }
  }
}
